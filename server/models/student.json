{
  "name": "student",
  "plural": "students",
  "base": "User",
  "idInjection": true,
  "options": {
    "validateUpsert": true
  },
  "properties": {
    "lastname": {
      "type": "string"
    },
    "firstname": {
      "type": "string"
    },
    "email": {
      "type": "string",
      "required": true
    },
    "birthdate": {
      "type": "date"
    },
    "studentId": {
      "type": "string"
    },
    "password": {
      "type": "string",
      "required": true
    },
    "cardStatus": {
      "type": "string"
    },
    "Ycoin": {
      "type": "number",
      "default": 0
    },
    "picture": {
      "type": "string"
    },
    "formation": {
      "type": "string"
    },
    "phone": {
      "type": "number"
    }
  },
  "validations": [],
  "relations": {
    "commentaireEvents": {
      "type": "hasMany",
      "model": "CommentaireEvent",
      "foreignKey": ""
    },
    "badges": {
      "type": "hasMany",
      "model": "Badge",
      "foreignKey": ""
    },
    "droits": {
      "type": "hasMany",
      "model": "Droit",
      "foreignKey": ""
    },
    "roles": {
      "type": "hasMany",
      "model": "Role",
      "foreignKey": "principalId",
      "through": "RoleMapping"
    }
  },
  "acls": [
    {
      "accessType": "*",
      "principalType": "ROLE",
      "principalId": "$authenticated",
      "permission": "ALLOW"
    },
    {
      "accessType": "EXECUTE",
      "principalType": "ROLE",
      "principalId": "$everyone",
      "permission": "ALLOW",
      "property": "login"
    },
    {
      "accessType": "*",
      "principalType": "ROLE",
      "principalId": "$everyone",
      "permission": "ALLOW"
    }
  ],
  "methods": {}
}
